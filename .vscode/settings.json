{
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.fixAll.eslint": "explicit"
  },
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript"
  ],
  "eslint.alwaysShowStatus": true
}